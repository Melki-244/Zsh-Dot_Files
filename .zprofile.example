#Custom Aliases 
alias pip-R="rm -rf requirements.txt ; pip freeze > requirements.txt"
alias le="exa --icons"
alias li="exa --icons -la"
alias venv="source ./venv/bin/activate"
alias ssh-add-M="ssh-add ~/.ssh/MainKey_ed25519"
#Oh-my-zsh Plugins
plugins=(
    git
    zsh-autosuggestions
    zsh-syntax-highlighting
    ssh-agent
)

# User configuration Plugins
zstyle :omz:plugins:ssh-agent agent-forwarding yes
zstyle :omzplugins:ssh-agent helper ksshaskpass:
zstyle :omz:plugins:ssh-agent identities MainKey_ed25519



